    <ui:MultipleAssign sap2010:Annotation.AnnotationText="TotalVendor - Soma de todos os valores em BRL informados no campo &quot;TotalBrl&quot;.&#xD;&#xA;&#xD;&#xA;Discount - Desconto aplicado se TotalVendor for maior que o limite definido em &quot;RangeDiscount&quot;, usando o percentual de &quot;TaxDiscount&quot;.&#xD;&#xA;&#xD;&#xA;InvoiceGenerationDate - Data da invoice mais antiga (campo &quot;Date&quot;) somada a um número de dias configurado em &quot;InvoiceGenerationDays&quot;, formatada como MM/dd/yyyy.&#xD;&#xA;&#xD;&#xA;Tax - Taxa de imposto definida no dicionário &quot;TaxStates&quot; com base no estado do vendedor (campo &quot;State&quot;). Retorna vazio se não encontrado.&#xD;&#xA;&#xD;&#xA;FilePdf - Caminho completo do arquivo PDF temporário, gerado a partir do nome e ID do vendedor, com base nos parâmetros &quot;PathTemp&quot; e &quot;FilePdfName&quot;." DisplayName="Multiple Assign Init Vars" sap:VirtualizedContainerService.HintSize="479,494" sap2010:WorkflowViewState.IdRef="MultipleAssign_1">
      <ui:MultipleAssign.AssignOperations>
        <scg:List x:TypeArguments="ui:AssignOperation" Capacity="8">
          <ui:AssignOperation sap2010:WorkflowViewState.IdRef="AssignOperation_1">
            <ui:AssignOperation.To>
              <OutArgument x:TypeArguments="x:Double">[TotalVendor]</OutArgument>
            </ui:AssignOperation.To>
            <ui:AssignOperation.Value>
              <InArgument x:TypeArguments="x:Double">[in_TransactionItem.SpecificContent("TotalBrl").ToString.Split(","c).Select(Function(x) Double.Parse(x.Trim, System.Globalization.CultureInfo.InvariantCulture)).Sum]</InArgument>
            </ui:AssignOperation.Value>
          </ui:AssignOperation>
          <ui:AssignOperation sap2010:WorkflowViewState.IdRef="AssignOperation_2">
            <ui:AssignOperation.To>
              <OutArgument x:TypeArguments="x:String">[Discount]</OutArgument>
            </ui:AssignOperation.To>
            <ui:AssignOperation.Value>
              <InArgument x:TypeArguments="x:String" xml:space="preserve">[If(
	TotalVendor &gt; CDbl(in_Config("RangeDiscount")), 
	(TotalVendor * CDbl(in_Config("TaxDiscount"))).ToString, 
	String.Empty
)]</InArgument>
            </ui:AssignOperation.Value>
          </ui:AssignOperation>
          <ui:AssignOperation sap2010:WorkflowViewState.IdRef="AssignOperation_3">
            <ui:AssignOperation.To>
              <OutArgument x:TypeArguments="x:String">[InvoiceGenerationDate]</OutArgument>
            </ui:AssignOperation.To>
            <ui:AssignOperation.Value>
              <InArgument x:TypeArguments="x:String" xml:space="preserve">[(From dateStr In in_TransactionItem.SpecificContent("Date").ToString.Split(","c)
 Select DateTime.ParseExact(
     dateStr.Trim,
     {"dd/MM/yyyy", "dd-MMM-yy", "MM/dd/yyyy"},
     System.Globalization.CultureInfo.InvariantCulture,
     System.Globalization.DateTimeStyles.None
 )).OrderBy(Function(d) d).First().AddDays(CInt(in_Config("InvoiceGenerationDays"))).ToString("MM/dd/yyyy")]</InArgument>
            </ui:AssignOperation.Value>
          </ui:AssignOperation>
          <ui:AssignOperation sap2010:WorkflowViewState.IdRef="AssignOperation_4">
            <ui:AssignOperation.To>
              <OutArgument x:TypeArguments="x:String">[Tax]</OutArgument>
            </ui:AssignOperation.To>
            <ui:AssignOperation.Value>
              <InArgument x:TypeArguments="x:String" xml:space="preserve">[If(
	Newtonsoft.Json.JsonConvert.DeserializeObject(Of Dictionary(Of String, String))(in_Config("TaxStates").ToString).ContainsKey(in_TransactionItem.SpecificContent("State").ToString),
	Newtonsoft.Json.JsonConvert.DeserializeObject(Of Dictionary(Of String, String))(in_Config("TaxStates").ToString)(in_TransactionItem.SpecificContent("State").ToString),
	String.Empty
)]</InArgument>
            </ui:AssignOperation.Value>
          </ui:AssignOperation>
          <ui:AssignOperation sap2010:WorkflowViewState.IdRef="AssignOperation_5">
            <ui:AssignOperation.To>
              <OutArgument x:TypeArguments="x:String">[FilePdf]</OutArgument>
            </ui:AssignOperation.To>
            <ui:AssignOperation.Value>
              <InArgument x:TypeArguments="x:String" xml:space="preserve">[Path.Combine(
	DirectCast(in_Config("PathTemp"), System.String),
	String.Format(
		DirectCast(in_Config("FilePdfName"), System.String), 
		in_TransactionItem.SpecificContent("NameVendor").ToString, 
		in_TransactionItem.SpecificContent("IdVendor").ToString
	)
)]</InArgument>
            </ui:AssignOperation.Value>
          </ui:AssignOperation>
        </scg:List>
      </ui:MultipleAssign.AssignOperations>
